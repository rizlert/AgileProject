package basicJava_Projects;

public class Test {

	public static void main(String[] args) {
		Test ts = new Test ();
		ts.greet();
		System.out.println(ts.greet());

	}
	public boolean greet () {
		
		boolean red= true;
		if (red==true) {
			System.out.println("red");
		} 
		else {System.out.println("n/a");}
		return red;
	}
}
